{
  "name": "remoteit-headless",
  "version": "3.14.0-beta.1",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 run-s clean copy-and-lock compile build-frontend pack",
    "build-frontend": "cd ../frontend && npm run build && cp -R build/* ../backend/build",
    "copy-and-lock": "chmod +w src/sharedCopy/* && cp -R ../frontend/src/shared/* ./src/sharedCopy && chmod -w src/sharedCopy/* && cp ./src/cli-version.json ./build",
    "clean": "rimraf build && mkdir build",
    "compile": "tsc",
    "start": "node build/index.js",
    "test": "NODE_ENV=test jest",
    "pack": "npm-pack-all --output ../dist/remoteit-headless.tgz",
    "watch": "cross-env DEBUG_COLORS=1 FORCE_COLOR=1 run-p copy-* watch-*",
    "watch-compile": "tsc --watch",
    "watch-shared": "onchange '../frontend/src/shared/**/*' -- npm run copy-and-lock",
    "test-watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@airbrake/node": "^1.4.2",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "debug": "4.3.1",
    "detect-rpi": "^1.4.0",
    "dotenv": "^8.2.0",
    "escape-string-regexp": "^4.0.0",
    "express": "4.17.3",
    "is-electron": "^2.2.0",
    "is-elevated": "^3.0.0",
    "is-port-reachable": "^3.1.0",
    "netmask": "^2.0.2",
    "network": "^0.6.1",
    "plist": "^3.0.5",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "socket.io": "^2.5.0",
    "socketio-auth": "^0.1.1",
    "sudo-prompt": "^9.2.1",
    "win-ca": "^3.5.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "@types/plist": "^3.0.2",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.4",
    "@types/socket.io": "^2.1.13",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "npm-pack-all": "^1.12.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  },
  "bundleDependencies": [
    "@airbrake/node",
    "axios",
    "cors",
    "debug",
    "detect-rpi",
    "dotenv",
    "express",
    "fsevents",
    "is-electron",
    "is-elevated",
    "is-port-reachable",
    "netmask",
    "network",
    "plist",
    "rimraf",
    "semver",
    "socket.io",
    "socketio-auth",
    "sudo-prompt",
    "win-ca",
    "winston"
  ]
}
