{
  "name": "remoteit-headless",
  "version": "3.24.0-beta.5",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clean copy-* compile",
    "copy-frontend": "[ -d ../frontend/build ] && [ -d ./build ] && cp -r ../frontend/build/* ./build",
    "copy-shared": "chmod +w src/sharedCopy/* && cp -R ../frontend/src/shared/* ./src/sharedCopy && chmod -w src/sharedCopy/*",
    "clean": "rimraf build && mkdir build",
    "compile": "tsc",
    "start": "node build/index.js",
    "test": "NODE_ENV=test jest",
    "pack": "npm-pack-all --output ./dist/remoteit-headless.tgz",
    "watch": "cross-env DEBUG_COLORS=1 FORCE_COLOR=1 run-p copy-* watch-*",
    "watch-compile": "tsc --watch",
    "watch-shared": "onchange '../frontend/src/shared/**/*' -- npm run copy-shared",
    "test-watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@airbrake/node": "^2.1.8",
    "@vscode/sudo-prompt": "^9.3.1",
    "axios": "^1.6.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "detect-rpi": "^1.4.0",
    "dotenv": "^16.3.1",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.18.2",
    "is-electron": "^2.2.2",
    "is-elevated": "^3.0.0",
    "is-port-reachable": "^3.1.0",
    "lodash.isequal": "^4.5.0",
    "netmask": "^2.0.2",
    "network": "^0.6.1",
    "plist": "^3.1.0",
    "rimraf": "^3.0.2",
    "semver": "^7.5.4",
    "socket.io": "^4.7.2",
    "socketio-auth": "^0.1.1",
    "win-ca": "^3.5.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^20.9.0",
    "@types/plist": "^3.0.5",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.5.5",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "npm-pack-all": "^1.12.7",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "files": [
    "build",
    "scripts",
    "ssl"
  ]
}
