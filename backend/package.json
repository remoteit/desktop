{
  "name": "remoteit-headless",
  "version": "3.15.0-alpha.0",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 run-s clean copy-and-lock compile build-frontend pack",
    "build-frontend": "cd ../frontend && npm run build && cp -R build/* ../backend/build",
    "copy-and-lock": "chmod +w src/sharedCopy/* && cp -R ../frontend/src/shared/* ./src/sharedCopy && chmod -w src/sharedCopy/* && cp ./src/cli-version.json ./build",
    "clean": "rimraf build && mkdir build",
    "compile": "tsc",
    "start": "node build/index.js",
    "test": "NODE_ENV=test jest",
    "pack": "npm-pack-all --output ../dist/remoteit-headless.tgz",
    "watch": "cross-env DEBUG_COLORS=1 FORCE_COLOR=1 run-p copy-* watch-*",
    "watch-compile": "tsc --watch",
    "watch-shared": "onchange '../frontend/src/shared/**/*' -- npm run copy-and-lock",
    "test-watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@airbrake/node": "^2.1.8",
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "detect-rpi": "^1.4.0",
    "dotenv": "^16.0.3",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.18.2",
    "is-electron": "^2.2.1",
    "is-elevated": "^3.0.0",
    "is-port-reachable": "^3.1.0",
    "netmask": "^2.0.2",
    "network": "^0.6.1",
    "plist": "^3.0.6",
    "punycode": "^2.3.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.8",
    "socket.io": "^4.5.4",
    "socketio-auth": "^0.1.1",
    "sudo-prompt": "^9.2.1",
    "win-ca": "^3.5.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@types/plist": "^3.0.2",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.13",
    "cross-env": "^7.0.3",
    "jest": "^29.4.1",
    "npm-pack-all": "^1.12.7",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.8.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "bundleDependencies": [
    "@airbrake/node",
    "axios",
    "cors",
    "debug",
    "detect-rpi",
    "dotenv",
    "express",
    "fsevents",
    "is-electron",
    "is-elevated",
    "is-port-reachable",
    "netmask",
    "network",
    "plist",
    "rimraf",
    "semver",
    "socket.io",
    "socketio-auth",
    "sudo-prompt",
    "win-ca",
    "winston"
  ]
}
