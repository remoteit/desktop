{
  "name": "remoteit-headless",
  "version": "2.5.24-alpha.0",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clean compile build-frontend pack",
    "build-frontend": "cd ../frontend && npm run build && cp -R build/* ../backend/build",
    "clean": "rimraf build/*",
    "compile": "tsc",
    "start": "node build/index.js",
    "test": "NODE_ENV=test jest",
    "pack": "npm-pack-all --output ../dist/remoteit-headless.tgz",
    "watch": "cross-env DEBUG_COLORS=1 FORCE_COLOR=1 run-p watch-*",
    "watch-compile": "tsc --watch",
    "test-watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@airbrake/node": "^1.0.6",
    "cors": "^2.8.5",
    "debug": "4.1.1",
    "detect-rpi": "^1.3.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "is-elevated": "^3.0.0",
    "netmask": "^1.0.6",
    "network": "^0.5.0",
    "portfinder": "^1.0.25",
    "remote.it": "^0.44.1",
    "rimraf": "^3.0.0",
    "semver": "^7.1.3",
    "socket.io": "^2.3.0",
    "socketio-auth": "^0.1.1",
    "sudo-prompt": "^9.1.1",
    "tmp": "^0.1.0",
    "universal-analytics": "0.4.20",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/auto-launch": "5.0.1",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/electron": "1.6.10",
    "@types/express": "^4.17.3",
    "@types/http-proxy": "^1.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/rimraf": "^2.0.3",
    "@types/semver": "^7.1.0",
    "@types/socket.io": "^2.1.4",
    "@types/tmp": "^0.1.0",
    "@types/universal-analytics": "^0.4.3",
    "cross-env": "^7.0.2",
    "jest": "^25.1.0",
    "npm-pack-all": "^1.11.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
