{
  "name": "remoteit-desktop",
  "version": "2.4.11",
  "private": true,
  "main": "build/index.js",
  "productName": "remote.it",
  "scripts": {
    "publish": "NODE_ENV=production run-s clean compile copy-* publish-*",
    "publish-frontend": "npm run build-frontend",
    "publish-backend": "electron-builder -wlm -p always",
    "build": "NODE_ENV=production run-s clean compile copy-* build-*",
    "build-frontend": "cd ../frontend && npm run build && cp -R build/* ../backend/build",
    "build-backend": "electron-builder -l",
    "copy-images": "cp -R ./src/images ./build/",
    "copy-entitlements": "cp entitlements.mac.plist build/",
    "clean": "rimraf build/* dist/*",
    "compile": "tsc",
    "electron": "cross-env NODE_ENV=development electron .",
    "test": "NODE_ENV=test jest",
    "start": "DEBUG_COLORS=1 FORCE_COLOR=1 run-p watch-*",
    "postinstall": "electron-builder install-app-deps",
    "watch-build": "tsc --watch",
    "watch-electron": "onchange -i -k 'build/**/*' -- npm run electron",
    "watch-images": "onchange -i -k 'src/images/**/*' -- npm run copy-images",
    "test-watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@types/auto-launch": "5.0.1",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/electron": "1.6.10",
    "@types/express": "^4.17.2",
    "@types/http-proxy": "^1.17.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/semver-compare": "^1.0.1",
    "@types/socket.io": "^2.1.4",
    "@types/tmp": "^0.1.0",
    "@types/universal-analytics": "^0.4.3",
    "cross-env": "^6.0.3",
    "electron": "^7.1.8",
    "electron-builder": "^22.2.0",
    "electron-notarize": "^0.2.1",
    "electron-osx-sign": "^0.4.15",
    "fsevents": "^2.1.2",
    "jest": "^24.9.0",
    "npm-run-all": "4.1.5",
    "onchange": "^6.1.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@airbrake/node": "^1.0.6",
    "cors": "^2.8.5",
    "debug": "4.1.1",
    "detect-rpi": "^1.3.0",
    "dotenv": "^8.2.0",
    "electron-updater": "^4.2.0",
    "express": "4.17.1",
    "netmask": "^1.0.6",
    "network": "^0.4.1",
    "portfinder": "^1.0.25",
    "remote.it": "^0.44.1",
    "semver-compare": "^1.0.0",
    "socket.io": "^2.3.0",
    "socketio-auth": "^0.1.1",
    "sudo-prompt": "^9.1.1",
    "tmp": "^0.1.0",
    "universal-analytics": "0.4.20",
    "winston": "3.2.1"
  },
  "homepage": "https://remote.it",
  "repository": {
    "url": "git+ssh://git@github.com/remoteit/desktop.git",
    "type": "git"
  },
  "license": "MIT",
  "author": {
    "name": "remote.it",
    "email": "support@remote.it"
  },
  "build": {
    "appId": "it.remote.desktop",
    "afterSign": "scripts/notarize.js",
    "copyright": "remot3.it, Inc",
    "productName": "remoteit-desktop",
    "artifactName": "${productName}-${arch}.${ext}",
    "publish": [
      "github"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "deleteAppDataOnUninstall": true,
      "shortcutName": "remote.it"
    },
    "linux": {
      "category": "Network",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "armv7l",
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "armv7l",
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "../frontend/build/**/*"
    ]
  }
}
